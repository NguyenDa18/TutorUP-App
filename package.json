{
  "name": "tutorup",
  "version": "1.0.0",
  "description": "Social Network for University of Portland students to find tutors",
  "main": "server.js",
  "engines": {
    "node": "10.12.0"
  },
  "scripts": {
    "test": "mocha --timeout 10000",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client-install": "npm install --prefix client",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TutorUP/TutorUP-App.git"
  },
  "keywords": [
    "react",
    "express",
    "tutor",
    "mongo",
    "node"
  ],
  "author": "Danh Nguyen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TutorUP/TutorUP-App/issues"
  },
  "homepage": "https://github.com/TutorUP/TutorUP-App#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "express-jwt-authz": "^1.0.0",
    "express-session": "^1.15.6",
    "helmet": "^3.15.0",
    "jsonwebtoken": "^8.4.0",
    "jwks-rsa": "^1.3.0",
    "mongoose": "^5.4.0",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-auth0": "^1.1.0",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "sinon": "^7.2.2",
    "sinon-mongoose": "^2.2.1",
    "supertest": "^3.4.1",
    "validator": "^10.9.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9"
  }
}
